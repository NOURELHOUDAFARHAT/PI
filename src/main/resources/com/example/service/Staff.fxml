    <children>
        <AnchorPane layoutX="-58.0" prefHeight="400.0" prefWidth="700.0">
            <children>

                <TableView fx:id="StaffView" layoutX="63.0" layoutY="98.0" prefHeight="200.0" prefWidth="323.0">
                    <columns>
                        <TableColumn fx:id="I" prefWidth="63.600006103515625" text="Staff Id" />
                        <TableColumn fx:id="N" prefWidth="105.20000457763672" text="Name" />
                        <TableColumn fx:id="J" prefWidth="82.0" text="Job" />
                        <TableColumn fx:id="S" prefWidth="71.0" text="Score" />

                    </columns>
                </TableView>
                <Label layoutX="479.0" layoutY="95.0" text="Add a new Staff">
